{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/patients/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "patientservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/patients/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/notes/{everything}",
      "UpstreamPathTemplate": "/notes/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "notesservice",
          "Port": 8080
        }
      ],
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/risk/{patientId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "riskservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/risk/{patientId}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5001"
  }
}
